<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane opacity="0.95" prefHeight="286.0" prefWidth="225.0" style="-fx-background-repeat: no-repeat;" styleClass="body" stylesheets="@LoginStyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LogInController">
    <VBox alignment="CENTER" layoutX="4.0" prefHeight="298.0" prefWidth="221.0">
      <Label styleClass="logInLabel" text="LOG IN" textFill="#000000cd">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <font>
            <Font name="Consolas Bold" size="20.0" />
         </font>
      </Label>
        <Label fx:id="message" prefHeight="29.0" prefWidth="463.0" style="visibility: false;" styleClass="message">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
        <AnchorPane>
            <JFXTextField fx:id="logInField" labelFloat="true" prefHeight="25.0" prefWidth="181.0" promptText="Log In" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <font>
               <Font name="Gabriola" size="22.0" />
            </font>
            <styleClass>
               <String fx:value="logInField" />
               <String fx:value="input" />
            </styleClass>
            <opaqueInsets>
               <Insets />
            </opaqueInsets></JFXTextField>
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
        </AnchorPane>
        <AnchorPane>
            <JFXTextField fx:id="passwordField" labelFloat="true" prefHeight="25.0" prefWidth="181.0" promptText="Password" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <font>
               <Font name="Gabriola" size="22.0" />
            </font>
            <styleClass>
               <String fx:value="passwordField" />
               <String fx:value="input" />
            </styleClass></JFXTextField>
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
        </AnchorPane>
      <HBox>
         <children>
              <Button fx:id="logInButton" styleClass="logInButton button" text="Log In">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font size="16.0" />
               </font></Button>
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="cancelButton" styleClass="cancelButton button" text="Cancel">
               <font>
                  <Font size="16.0" />
               </font></Button>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </HBox>
      <padding>
         <Insets left="20.0" right="20.0" top="15.0" />
      </padding>
    </VBox>

</AnchorPane>
